{
    "name": "halloverden/symfony-access-definitions-bundle",
    "description": "Access definitions for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HalloVerden\\AccessDefinitionsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HalloVerden\\AccessDefinitionsBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Roar Furunes",
            "email": "tom.roar@halloverden.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5.4|^6.0",
        "jms/metadata": "^2.6",
        "symfony/yaml": "^5.4|^6.0",
        "halloverden/symfony-voter-bundle": "^2.1",
        "symfony/expression-language": "^5.4|^6.0",
        "halloverden/symfony-jwt-authenticator-bundle": "^2.1",
        "jms/serializer": "^3.18",
        "symfony/validator": "^5.4|^6.0",
        "halloverden/symfony-oidc-contracts": "^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4|^6.1"
    }
}
